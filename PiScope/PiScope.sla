            <ITEXT FONT="FreeSans Bold" FONTSIZE="12" CH=" "/>
            <para/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" #!/bin/bash"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT CH=" # A script to setup the Raspberry Pi for a kernel build"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" FV=`zgrep &quot;* firmware as of&quot; /usr/share/doc/raspberrypi-bootloader/changelog.Debian.gz | head -1 | awk '{ print $5 }'`"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" mkdir -p k_tmp/linux"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" wget https://raw.github.com/raspberrypi/firmware/$FV/extra/git_hash -O k_tmp/git_hash"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" wget https://raw.github.com/raspberrypi/firmware/$FV/extra/Module.symvers -O k_tmp/Module.symvers"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" HASH=`cat k_tmp/git_hash`"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" wget -c https://github.com/raspberrypi/linux/tarball/$HASH -O k_tmp/linux.tar.gz"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" cd k_tmp"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" tar -xzf linux.tar.gz"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" KV=`uname -r` "/>
            <para LINESPMode="0" LINESP="10"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" sudo mv raspberrypi-linux* /usr/src/linux-source-$KV"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" sudo ln -s /usr/src/linux-source-$KV /lib/modules/$KV/build"/>
            <para LINESPMode="0" LINESP="10"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" sudo cp Module.symvers /usr/src/linux-source-$KV/"/>
            <para LINESPMode="1" LINESP="6"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" sudo zcat /proc/config.gz &gt; /usr/src/linux-source-$KV/.config"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" cd /usr/src/linux-source-$KV/"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" sudo make oldconfig"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" sudo make prepare"/>
            <para LINESPMode="1"/>
            <ITEXT FONT="Monaco Regular" FONTSIZE="7" CH=" sudo make scripts"/>
            <para LINESPMode="1"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
